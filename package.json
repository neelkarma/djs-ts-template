{
  "name": "djs-ts-template",
  "type": "module",
  "scripts": {
    "build": "tsdown --entry ./src/index.ts",
    "watch": "tsdown --entry ./src/index.ts --watch",
    "start": "node ./dist/index.js",
    "dev": "tsdown --entry ./src/index.ts --watch --onSuccess \"node ./dist/index.js\"",
    "register": "tsdown --entry ./src/register.ts && node ./dist/register.js"
  },
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "tsdown": "^0.12.4"
  },
  "dependencies": {
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0"
  }
}
