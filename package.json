{
  "name": "djs-ts-template",
  "scripts": {
    "dev": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "build": "tsc",
    "deploy:dev": "tsc && node ./scripts/deploy.dev.js",
    "deploy:prod": "tsc && node ./scripts/deploy.prod.js",
    "undeploy:dev": "node ./scripts/undeploy.dev.js",
    "undeploy:prod": "node ./scripts/undeploy.prod.js"
  },
  "devDependencies": {
    "@types/node": "^17.0.5",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@discordjs/builders": "^0.10.0",
    "@discordjs/rest": "^0.2.0-canary.0",
    "discord-api-types": "^0.26.0",
    "discord.js": "^13.5.0",
    "dotenv": "^10.0.0"
  }
}
